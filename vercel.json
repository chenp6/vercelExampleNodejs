{
    "version": 2,
    "builds": [{
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [    {
         "src": "/users/([^/]+)/([^/]+)",
         "dest": "/api/users/:email/:name"
    },
    {
        "src": "/(.+)",
        "dest": "/"
    }
    ]
}
